{
  "name": "skiller",
  "version": "0.1.0",
  "description": "Simple tool to visualize professional skills",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "cross-env rimraf dist",
    "dev": "nodemon --exec \"ts-node $1\"",
    "build:browser": "cross-env webpack --config webpack/browser.ts",
    "build:ssr": "cross-env webpack --config webpack/ssr.ts",
    "build": "yarn clean && yarn build:browser && yarn build:ssr",
    "deploy": "now --no-clipboard --token=$NOW_TOKEN"
  },
  "dependencies": {
    "@loadable/component": "^5.5.0",
    "@loadable/server": "^5.5.0",
    "cross-fetch": "^3.0.0",
    "graphql": "^14.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.6.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "require-from-string": "^2.0.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@loadable/babel-plugin": "^5.5.0",
    "@loadable/webpack-plugin": "^5.5.0",
    "@types/graphql": "^14.0.5",
    "@types/loadable__component": "^5.2.0",
    "@types/loadable__server": "^5.2.0",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/require-from-string": "^1.2.0",
    "@types/styled-components": "^4.1.6",
    "@types/webpack": "^4.4.24",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "fs": "^0.0.1-security",
    "graphql-tag": "^2.10.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "now": "^13.1.2",
    "path": "^0.12.7",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "url-loader": "^1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
